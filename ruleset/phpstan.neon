parameters:
    level: max
    reportUnmatchedIgnoredErrors: false
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false

    excludes_analyse:
        # Makes PHPStan crash
        - ../src/DependencyInjection/Configuration.php
        - ../src/Migrations/
        - ../tests/PHPUnit

        # Test dependencies
        - ../tests/Application

        # PHPSpec
        - ../spec

    ignoreErrors:
        # ignore false positive
